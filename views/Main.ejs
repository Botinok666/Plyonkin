	</head>
	<body onload='pageLoaded()'>
		<div class="logo">
			<% include header %>
			<div class="container-fluid">
					<div class="col-md-12">
						<section>
							<article class="main_news"/>
						</section>
					</div>
				</div>
		</div>
		<div class="container-fluid">
			<div class="rowMain">
				
					<div id="leftCol" class="column"></div>
					<div id="rightCol" class="column"></div>
					<div class="popular">
					</div>
					
			</div>
		</div>
		<% include footer %>
	</body>
